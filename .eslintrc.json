{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "eslint:recommended",
    "@sumerian"
  ],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "eol-last":"error",
    "implicit-arrow-linebreak": "off",
    "object-curly-newline": [
      "error",
      {
        "consistent": true,
        "multiline": true
      }
    ]
  }
}